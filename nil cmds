
local ScreenGui = Instance.new("ScreenGui")
local CommandBox = Instance.new("TextBox")
local MainFrame = Instance.new("ScrollingFrame")
local UIListLayout = Instance.new("UIListLayout")
local copyme = Instance.new("TextButton")
local description = Instance.new("TextLabel")
local UICorner = Instance.new("UICorner")


ScreenGui.Parent = game:GetService("CoreGui")

CommandBox.Name = "CommandBox"
CommandBox.Parent = ScreenGui
CommandBox.BackgroundColor3 = Color3.fromRGB(58, 58, 58)
CommandBox.BorderColor3 = Color3.fromRGB(53, 53, 53)
CommandBox.BorderSizePixel = 0
CommandBox.Position = UDim2.new(0.854014575, 0, 0.65733695, 0)
CommandBox.Size = UDim2.new(0, 160, 0, 19)
CommandBox.ClearTextOnFocus = false
CommandBox.Font = Enum.Font.SourceSans
CommandBox.PlaceholderText = "..."
CommandBox.Text = ""
CommandBox.TextColor3 = Color3.fromRGB(255, 255, 255)
CommandBox.TextSize = 14.000

MainFrame.Name = "MainFrame"
MainFrame.Parent = ScreenGui
MainFrame.Active = true
MainFrame.BackgroundColor3 = Color3.fromRGB(54, 54, 54)
MainFrame.BorderSizePixel = 0
MainFrame.Position = UDim2.new(0.854014575, 0, 0.696259737, 0)
MainFrame.Size = UDim2.new(0, 160, 0, 153)
MainFrame.CanvasSize = UDim2.new(0, 0, 5000, 0)
MainFrame.ScrollBarThickness = 5
MainFrame.VerticalScrollBarPosition = Enum.VerticalScrollBarPosition.Left

UIListLayout.Parent = MainFrame
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout.Padding = UDim.new(0, 10)

copyme.Name = "copyme"
copyme.Parent = MainFrame
copyme.BackgroundColor3 = Color3.fromRGB(53, 53, 53)
copyme.BackgroundTransparency = 1.000
copyme.BorderSizePixel = 0
copyme.Position = UDim2.new(-3.17499995, 0, -1.6316396, 0)
copyme.Size = UDim2.new(0, 160, 0, 14)
copyme.Visible = false
copyme.Font = Enum.Font.SourceSans
copyme.TextColor3 = Color3.fromRGB(255, 255, 255)
copyme.TextSize = 14.000
copyme.TextWrapped = true

description.Name = "description"
description.Parent = copyme
description.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
description.BorderSizePixel = 0
description.Position = UDim2.new(0, 0, 0.992061079, 0)
description.Size = UDim2.new(0, 162, 0, 21)
description.Visible = false
description.ZIndex = 2
description.Font = Enum.Font.SourceSans
description.TextColor3 = Color3.fromRGB(255, 255, 255)
description.TextSize = 14.000
description.TextWrapped = true

UICorner.CornerRadius = UDim.new(0, 3)
UICorner.Parent = description

-- Scripts:

local function DUQL_fake_script() -- CommandBox.LocalScript 
	local script = Instance.new('LocalScript', CommandBox)

	local commands = {
		["print"] = {
			function(args)
				print(args)
			end,
			desc = "Print args",
		},
		["nolag"] = {
			function()
				for i, v in pairs(workspace:GetDescendants()) do
					if v:IsA("Decal") then
						v:Destroy()
					end
				end
			end,
			desc = "Enables no lag mode"
		},
		["blockhead"] = {
			function()
				local player = game:GetService("Players").LocalPlayer
				local char = player.Character or player.CharacterAdded:Wait()
				char:WaitForChild("Head").Mesh:Destroy()
			end,
			desc = "Turns your head into a block"
		},
		["blockhats"] = {
			function()
				local player = game:GetService("Players").LocalPlayer
				local char = player.Character or player.CharacterAdded:Wait()
				for i, v in pairs(char:GetDescendants()) do
					if v:IsA("Accessory") then
						v.Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
					end
				end
			end,
			desc = "Turns your hats into blocks"
		},
		["walkspeed"] = {
			function(args)
				local player = game:GetService("Players").LocalPlayer
				local char = player.Character or player.CharacterAdded:Wait()
				local human = char:FindFirstChild("Humanoid")
	
				human.WalkSpeed = args
			end,
			desc = "Changes your walkspeed"
		},
		["hipheight"] = {
			function(args)
				local player = game:GetService("Players").LocalPlayer
				local char = player.Character or player.CharacterAdded:Wait()
				local human = char:FindFirstChild("Humanoid")
	
				human.HipHeight = args			
			end,
			desc = "Changes your hipheight"
		},
		["fly"] = {
			function()
				repeat wait()
				until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("Torso") and game.Players.LocalPlayer.Character:findFirstChild("Humanoid")
				local mouse = game.Players.LocalPlayer:GetMouse()
				repeat wait() until mouse
				local plr = game.Players.LocalPlayer
				local torso = plr.Character.Torso
				local flying = true
				local deb = true
				local ctrl = {f = 0, b = 0, l = 0, r = 0}
				local lastctrl = {f = 0, b = 0, l = 0, r = 0}
				local maxspeed = 50
				local speed = 0
	
				function Fly()
					local bg = Instance.new("BodyGyro", torso)
					bg.P = 9e4
					bg.maxTorque = Vector3.new(9e9, 9e9, 9e9)
					bg.cframe = torso.CFrame
					local bv = Instance.new("BodyVelocity", torso)
					bv.velocity = Vector3.new(0,0.1,0)
					bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
					repeat wait()
						plr.Character.Humanoid.PlatformStand = true
						if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then
							speed = speed+.5+(speed/maxspeed)
							if speed > maxspeed then
								speed = maxspeed
							end
						elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then
							speed = speed-1
							if speed < 0 then
								speed = 0
							end
						end
						if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then
							bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).Position) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed
							lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r}
						elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then
							bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).Position) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed
						else
							bv.velocity = Vector3.new(0,0.1,0)
						end
						bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0)
					until not flying
					ctrl = {f = 0, b = 0, l = 0, r = 0}
					lastctrl = {f = 0, b = 0, l = 0, r = 0}
					speed = 0
					bg:Destroy()
					bv:Destroy()
					plr.Character.Humanoid.PlatformStand = false
				end
				mouse.KeyDown:connect(function(key)
					if key:lower() == "e" then
						if flying then flying = false
						else
							flying = true
							Fly()
						end
					elseif key:lower() == "w" then
						ctrl.f = 1
					elseif key:lower() == "s" then
						ctrl.b = -1
					elseif key:lower() == "a" then
						ctrl.l = -1
					elseif key:lower() == "d" then
						ctrl.r = 1
					end
				end)
				mouse.KeyUp:connect(function(key)
					if key:lower() == "w" then
						ctrl.f = 0
					elseif key:lower() == "s" then
						ctrl.b = 0
					elseif key:lower() == "a" then
						ctrl.l = 0
					elseif key:lower() == "d" then
						ctrl.r = 0
					end
				end)
				Fly()
			end,
	
			desc = "Changes your hipheight"
		},
		["swim"] = {
			function(args)
				local human = game.Players.LocalPlayer.Character.Humanoid
				while true do
					wait()
					human:ChangeState(Enum.HumanoidStateType.Swimming)
				end
			end,
			desc = "Makes you able to swim in air"
		},
		["blocktools"] = {
			function()
				local player = game:GetService("Players").LocalPlayer
				local char = player.Character or player.CharacterAdded:Wait()
				local tool = char:FindFirstChildWhichIsA("Tool")
				for i, v in pairs(tool:GetDescendants()) do
					if v:IsA("SpecialMesh") then
						v:Destroy()
					end
				end
			end,
			desc = "Hold the tool to make it blocky"
		},
		
	}
	
	local box = script.Parent
	local mainframe = box.Parent.MainFrame
	local copyme = mainframe.copyme
	local uis = game:GetService("UserInputService")
	
	uis.InputBegan:Connect(function(key, chat)
		if not chat and key.KeyCode == Enum.KeyCode.Semicolon then
			box:CaptureFocus()
		end
	end)
	
	box.FocusLost:Connect(function(enter)
		if enter then
			local input = box.Text
			local args = string.split(input, " ")
			print(args)
			local command = commands[args[1]][1]
	
			if command then
				command(unpack(args, 2))
			end
		end
	end)
	
	
	for i, v in pairs(commands) do
		local clone = copyme:clone()
		clone.Name = i
		clone.Text = i
		clone.Visible = true
		clone.Parent = mainframe
		clone.description.Text = v.desc
		clone.MouseButton1Click:Connect(function()
			box.Text = clone.Text
		end)
		clone.MouseEnter:Connect(function()
			clone.description.Visible = true
		end)
		clone.MouseLeave:Connect(function()
			clone.description.Visible = false
		end)
	end
end
coroutine.wrap(DUQL_fake_script)()
