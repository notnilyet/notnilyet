local function BDCT_fake_script()
	local script = Instance.new('LocalScript', CommandBox)
	
	local ui = script.Parent.Parent
	local box = script.Parent
	local mainframe = ui.MainFrame
	local sounds = ui.Sounds
	local copyme = mainframe.copyme
	local uis = game:GetService("UserInputService")
	local twserv = game:GetService("TweenService")
	local info = TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0)
	
	local players = game:GetService("Players")
	local player = players.LocalPlayer
	local char = player.Character or player.CharacterAdded:Wait()
	local human = char:WaitForChild("Humanoid")
	local animator = human:FindFirstChildWhichIsA("Animator")
	local rand = math.random()
	
	local globalfunction = {
		createnotif = function(plate, text, duration)
			local tweenservice = game:GetService("TweenService")
			local template = script.Parent.Parent.NotificationTemplate
			local info = TweenInfo.new(1,Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0)
			local oldpos = UDim2.new(0.755, 0, 1.2, 0)
			local newpos = UDim2.new(0.756, 0, 0.86, 0)
	
			template.Plate.Text = plate
			template.TextLabel.Text = text
	
			template.Visible = true
			local up = tweenservice:Create(template, info, {Position = newpos})
			up:Play()
			up.Completed:Wait()
			wait(tonumber(duration))
			local down = tweenservice:Create(template, info, {Position = oldpos})
			down:Play()
			down.Completed:Wait()
			template.Visible = false
		end,
		predict = function(Table, input)
			local results = {}
	
			for i,v in pairs(Table) do
				if i:sub(1,#input):lower() == input:lower() then 
					table.insert(results,i) 
				end
			end
	
			if #results >= 1 then
				return results
			else
				return {}
			end
		end,
		playsound = function(sound, bool, bool1)
			local sound1 = sounds:FindFirstChild(sound)
			if sound1 then
				sound1:Play()
				if bool == true or bool1 == "true" then
					sound1.Ended:Connect(function()
						sound1:Destroy()
					end)
				end
				if bool1 == true or bool1 == "true" then
					sound1.Looped = true
				end
			else
				local h = Instance.new("Sound")
				h.SoundId = "rbxassetid://"..sound
				h.Parent = sounds
				h.Name = "Sound"..sound
				h:Play()
				if bool == true or bool == "true" then
					h.Ended:Connect(function()
						h:Destroy()
					end)
				end
				if bool1 == true or bool1 == "true" then
					h.Looped = true
				end
			end
		end,
		stopsound = function(sound)
			local sound1 = sounds:FindFirstChild(sound)
			if sound1 then
				sound1:Stop()
			end
		end,
		removesound = function(sound)
			local sound1 = sounds:FindFirstChild(sound)
			if sound1 then
				sound1:Destroy()
			end
		end,
	}
	
	local commands = {
		["print"] = {
			function(args)
				print(args)
			end,
			desc = "Print args",
		},
		["nolag"] = {
			function()
				for i, v in pairs(workspace:GetDescendants()) do
					if v:IsA("Decal") then
						v:Destroy()
					end
				end
			end,
			desc = "Enables no lag mode"
		},
		["blockhead"] = {
			function()
				char:WaitForChild("Head").Mesh:Destroy()
			end,
			desc = "Turns your head into a block"
		},
		["blockhats"] = {
			function()
				for i, v in pairs(char:GetDescendants()) do
					if v:IsA("Accessory") then
						v.Handle:FindFirstChildOfClass("SpecialMesh"):Destroy()
					end
				end
			end,
			desc = "Turns your hats into blocks"
		},
		["walkspeed"] = {
			function(args)
				human.WalkSpeed = args
			end,
			desc = "Changes your walkspeed"
		},
		["hipheight"] = {
			function(args)
	
				human.HipHeight = args			
			end,
			desc = "Changes your hipheight"
		},
		["fly"] = {
			function()
				game:GetService("Players").LocalPlayer:Kick("skill issue")
			end,
	
			desc = "Unuseable due to skill issue of the creator"
		},
		["swim"] = {
			function(args)
				while true do
					wait()
					human:ChangeState(Enum.HumanoidStateType.Swimming)
				end
			end,
			desc = "Makes you able to swim in air"
		},
		["blocktools"] = {
			function()
				local tool = char:FindFirstChildWhichIsA("Tool")
				for i, v in pairs(tool:GetDescendants()) do
					if v:IsA("SpecialMesh") then
						v:Destroy()
					end
				end
			end,
			desc = "Hold the tool to make it blocky"
		},
		["gripup"] = {
			function(args, args1, args2)
				local tool = char:FindFirstChildWhichIsA("Tool")
	
				tool.GripUp = Vector3.new(args, args1, args2)
			end,
			desc = "Changes tool GripUp value"
		},
		["gripforward"] = {
			function(args, args1, args2)
				local tool = char:FindFirstChildWhichIsA("Tool")
	
				tool.GripForward = Vector3.new(args, args1, args2)
			end,
			desc = "Changes tool GripForward value"
		},
		["grippos"] = {
			function(args, args1, args2)
				local tool = char:FindFirstChildWhichIsA("Tool")
	
				tool.GripPos = Vector3.new(args, args1, args2)
			end,
			desc = "Changes tool GripUp value"
		},
		["gripright"] = {
			function(args, args1, args2)
				local tool = char:FindFirstChildWhichIsA("Tool")
	
				tool.GripRight = Vector3.new(args, args1, args2)
			end,
			desc = "Changes tool GripUp value"
		},
		["drop"] = {
			function()
				local tool = char:FindFirstChildWhichIsA("Tool")
				local grip = char["Right Arm"]:FindFirstChild("RightGrip")
	
				if grip then
					grip:Destroy()
					tool.Parent = workspace
				end
			end,
			desc = "Drops your tool"
		},
		["createnotify"] = {
			function(plate, text, duration)
				globalfunction.createnotif(plate, text, duration)
			end,
			desc = "Creates a notification"
		},
		["placeid"] = {
			function()
				globalfunction.createnotif("Place Id", game.PlaceId, 3)
			end,
			desc = "Returns place id"
		},
		["jobid"] = {
			function()
				globalfunction.createnotif("Job Id", game.JobId, 3)
			end,
			desc = "Returns job id"
		},
		["placevers"] = {
			function()
				globalfunction.createnotif("Place version", game.PlaceVersion, 3)
			end,
			desc = "Returns place version"
		},
		["tp"] = {
			function(plr)
				local tpto = workspace:FindFirstChild(plr)
	
				char.PrimaryPart.CFrame = tpto.PrimaryPart.CFrame
			end,
			desc = "Teleports your character to a player"
		},
		["noclip"] = {
			function()
				local runservice = game:GetService("RunService")
	
				conn = runservice.Stepped:Connect(function()
					for i, v in pairs(char:GetDescendants()) do
						if v:IsA("BasePart") then
							v.CanCollide = false
						end
					end
				end)
			end,
			desc = "Makes your character able to clip through objects"
		},
		["unnoclip"] = {
			function()
				conn:Disconnect()
			end,
			desc = "Disables noclip"
		},
		["sit"] = {
			function()
				human.Sit = true
			end,
			desc = "Makes you sit"
		},
		["forcesit"] = {
			function()
				local runservice = game:GetService("RunService")
				sitconn = runservice.RenderStepped:Connect(function()
					human.Sit = true
				end)	
			end,
			desc = "Forces you to sit"
		},
		["unforcesit"] = {
			function()
				sitconn:Disconnect()
			end,
			desc = "Unforcesits you"
		},
		["playsound"] = {
			function(name, bool, bool1)
				globalfunction.playsound(name, bool, bool1)
			end,
			desc = "Plays a sound"
		},
		["stopsound"] = {
			function(sound)
				globalfunction.stopsound(sound)
			end,
			desc = "Stops a sound"
		},
		["removesound"] = {
			function(sound)
				globalfunction.removesound(sound)
			end,
			desc = "Removes a sound"
		},
		["close"] = {
			function()
				game:Shutdown()
			end,
			desc = "Closes your client"
		},
		["reset"] = {
			function()
				human:TakeDamage(math.huge)
			end,
			desc = "Resets your character"
		},
		["mvua"] = {
			function(arg)
				local player
				if arg then
					player = workspace:FindFirstChild(arg)
				else
					player = game:GetService("Players").LocalPlayer.Character
				end
				for i, v in pairs(workspace:GetDescendants()) do
					if v:IsA("Part") and v.Anchored == false then
						local mover = Instance.new("BodyPosition")
						mover.Position = player.PrimaryPart.Position
						mover.Parent = v
					end
				end
				for i, v in pairs(player:GetDescendants()) do
					if v:IsA("BodyPosition") then
						v:Destroy()
					end
				end
			end,
			desc = "Moves unanchored parts"
		},
		["frua"] = {
			function()
				for i, v in pairs(workspace:GetDescendants()) do
					if v:IsA("BodyPosition") then
						v.Position = v.Parent.Position
					end
				end
			end,
			desc = "Freezes unanchored parts"
		},
		["dtua"] = {
			function()
				for i, v in pairs(workspace:GetDescendants()) do
					if v:IsA("BodyPosition") then
						v.P = math.huge
						v.D = math.huge
						v.Position = Vector3.new(math.huge, math.huge, math.huge)
					end
				end
			end,
			desc = "Destroys unanchored parts"
		},
		["hlua"] = {
			function()
				for i, v in pairs(workspace:GetDescendants()) do
					if v:IsA("BasePart") and v.Anchored == false then
						local clone = ui.Highlight:Clone()
						clone.Parent = v
						for _, gui in pairs(clone:GetChildren()) do
							gui.Adornee = v
						end
					end
				end
			end,
			desc = "Highlights unanchored parts"
		},
		["unhl"] = {
			function()
				for i, v in pairs(workspace:GetDescendants()) do
					if v:IsA("Folder") and v.Name == "Highlight" then
						v:Destroy()
					end
				end
			end,
			desc = "Unhighlights unanchored parts"
		},
		["checksound"] = {
			function()
				local h = game:GetService("SoundService").RespectFilteringEnabled
				globalfunction.createnotif("RespectFilteringEnabled", tostring(h), 5)
			end,
			desc = "Notifies RespectFilteringEnabled value"
		},
		["spectateplr"] = {
			function(arg)
				local cam = workspace.CurrentCamera
				cam.CameraSubject = workspace:FindFirstChild(arg).Humanoid
			end,
			desc = "Spectates arg"
		},
		["spectatepart"] = {
			function(arg)
				local cam = workspace.CurrentCamera
				cam.CameraSubject = workspace:FindFirstChild(arg)
			end,
			desc = "Spectates a part"
		},
		["unspectate"] = {
			function()
				local cam = workspace.CurrentCamera
				cam.CameraSubject = char.Humanoid
			end,
			desc = "Disables spectate"
		},
		["refresh"] = {
			function()
			
			end,
			desc = "lol"
		},
		["table.new"] = {
			function(name, value, valuev)
				local tabl = Instance.new("Folder")
				tabl.Name = name
				tabl.Parent = player
				local val = Instance.new("StringValue")
				val.Value = valuev
				val.Name = value
				val.Parent = tabl
			end,
			desc = "Creates a FAKE table"
		},
		["table.remove"] = {
			function(name)
				player:FindFirstChild(name):Destroy()
			end,
			desc = "Removes the FAKE table"
		},
		["table.freeze"] = {
			function(name)
				local tabl = player:FindFirstChild(name)
				tableconn = tabl.ChildAdded:Connect(function(child)
					child:Destroy()
				end)
			end,
			desc = "Freezes the FAKE table"
		},
		["table.unfreeze"] = {
			function()
				tableconn:Disconnect()
			end,
			desc = "Unfreees a table"
		},
		["value.new"] = {
			function(name, value, tablename)
				local val = Instance.new("StringValue")
				val.Value = value
				val.Name = name
				val.Parent = player:FindFirstChild(tablename)
			end,
			desc = "Creates a new value"
		},
		["value.remove"] = {
			function(tablename, valname)
				player:FindFirstChild(tablename)[valname]:Destroy()
			end,
			desc = "Destroys the value"
		},
		["nilchar"] = {
			function()
				char.Parent = nil
			end,
			desc = "Changes your character's parent to nil"
		},
		["unnilchar"] = {
			function()
				char.Parent = workspace
			end,
			desc = "Undoes nilchar"
		},
		["hlliving"] = {
			function()
				for _, v in pairs(workspace:GetChildren()) do
					if v:IsA("Model") and v:FindFirstChildWhichIsA("Humanoid") then
						for q, part in pairs(v:GetChildren()) do
							if part:IsA("BasePart") then
								local clone = ui.Highlight:Clone()
								clone.Parent = part
								for x, v in pairs(clone:GetChildren()) do
									v.Adornee = part
								end
							end
						end
					end
				end
			end,
			desc = "Highlights living"
		},
		["getanims"] = {
			function()
				for i, v in pairs(game:GetDescendants()) do
					if v:IsA("Animation") then
						print(v.Name.." "..v.AnimationId)
					end
				end
			end,
			desc = "Gets all animations"
		},
		["playanim"] = {
			function(arg, looped)
				local a = Instance.new("Animation")
				a.AnimationId = arg
				a.Parent = ui.Animations
				local anim = animator:LoadAnimation(a)
				if looped == "true" then
					anim.Looped = true
				end
				anim:Play()
			end,
			desc = "Plays an animation"
		},
		["ping"] = {
			function(exact)
				if exact == "true" then
					globalfunction.createnotif("Ping", math.floor(player:GetNetworkPing()), 5)
				elseif exact == "false" then
					globalfunction.createnotif("Ping", player:GetNetworkPing(), 5)
				end
			end,
			desc = "Gets your ping"
		},
		["physfps"] = {
			function(exact)
				if exact == "true" then
					globalfunction.createnotif("Ping", math.floor(workspace:GetRealPhysicsFPS()), 5)
				elseif exact == "false" then
					globalfunction.createnotif("Ping", workspace:GetRealPhysicsFPS(), 5)
				end
			end,
			desc = "Gets physics FPS"
		},
		["realfps"] = {
			function(exact)
				if exact == "true" then
					globalfunction.createnotif("FPS", math.floor(1 / game:GetService("RunService").RenderStepped:Wait()), 5)
				elseif exact == "false" then
					globalfunction.createnotif("FPS", 1 / game:GetService("RunService").RenderStepped:Wait(), 5)
				end
			end,
			desc = "Gets your FPS"
		},
		["dex"] = {
			function()
				loadstring(game:HttpGetAsync("https://pastebin.com/raw/iuQPQq4M"))()
			end,
			desc = "Loads Dark DEX"
		},
		["audiologger"] = {
			function()
				loadstring(game:HttpGet('https://pastebin.com/raw/v7Usg709', true))()
			end,
			desc = "Loads audio logger"
		},
		["frosthook"] = {
			function()
				loadstring(game:HttpGet("https://raw.githubusercontent.com/Nootchtai/FrostHook_Spy/master/Spy.lua"))()
			end,
			desc = "Loads frost hook"
		},
		["frosthook"] = {
			function()
				loadstring(game:HttpGet("https://raw.githubusercontent.com/Nootchtai/FrostHook_Spy/master/Spy.lua"))()
			end,
			desc = "Loads frost hook"
		},
		["turtlespy"] = {
			function()
				loadstring(game:HttpGet("https://pastebin.com/raw/BDhSQqUU", true))()
			end,
			desc = "Loads turtle spy"
		},
		["netless"] = {
			function()
				loadstring(game:HttpGet("https://pastebin.com/raw/Kc5Wpt9s", true))()
			end,
			desc = "Loads simple netless"
		},
		["simradius"] = {
			function()
				sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius",math.huge)
				settings().Physics.AllowSleep = false
  					if setfflag then setfflag("PlayerNoAccessRadius","False")
				end
			end,
			desc = "Maxes out simradius"
		}
	}
	
	uis.InputBegan:Connect(function(key, chat)
		if not chat and key.KeyCode == Enum.KeyCode.Semicolon then
			box:CaptureFocus()
		end
	end)
	
	box.FocusLost:Connect(function(enter)
		if enter then
			local input = box.Text
			local args = string.split(input, " ")
			print(args)
			local command = commands[args[1]][1]
	
			if command then
				command(unpack(args, 2))
			end
		end
	end)
	
	
	box:GetPropertyChangedSignal("Text"):Connect(function()
		local availableCMDs = globalfunction.predict(commands, box.Text)
		for i, v in pairs(mainframe:GetChildren()) do
			if v:IsA("TextButton") then
				v.Visible = false
				for i1, v1 in pairs(availableCMDs) do
					if v.Text == v1 then
						v.Visible = true
					end
				end
			end
		end
	end)
	
	for i, v in pairs(commands) do
		local clone = copyme:clone()
		clone.Name = i
		clone.Text = i
		clone.Visible = true
		clone.Parent = mainframe
		clone.description.Text = v.desc
		clone.MouseButton1Click:Connect(function()
			box.Text = clone.Text
		end)
		clone.MouseEnter:Connect(function()
			local m = twserv:Create(clone.description, info, {TextTransparency = 0})
			local t = twserv:Create(clone.ButtonGradHolder, info, {BackgroundTransparency = 0})
			globalfunction.playsound("Click1", false, false)
			m:Play()
			t:Play()
			m.Completed:Wait()
			t.Completed:Wait()
		end)
		clone.MouseLeave:Connect(function()
			local m = twserv:Create(clone.description, info, {TextTransparency = 1})
			local t = twserv:Create(clone.ButtonGradHolder, info, {BackgroundTransparency = 1})
			m:Play()
			t:Play()
			m.Completed:Wait()
			t.Completed:Wait()
		end)
	end
end
coroutine.wrap(BDCT_fake_script)()
